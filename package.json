{
  "name": "@aneoconsultingfr/generate-next-version",
  "version": "0.0.4",
  "description": "Generate a new version using latest commit messages and following the semantic versioning",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag",
    "prepublishOnly": "nr build",
    "start": "nr dev && node cli.mjs",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/generate-next-version.git"
  },
  "keywords": [
    "version",
    "semver"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./cli.mjs",
  "files": [
    "dist",
    "*.mjs"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Est√©ban Soubiran <esoubiran@aneo.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneoconsulting/generate-next-version/issues"
  },
  "homepage": "https://github.com/aneoconsulting/generate-next-version#readme",
  "dependencies": {
    "c12": "^1.4.1",
    "cac": "^6.7.14",
    "changelogen": "^0.5.3",
    "changelogithub": "^0.12.11",
    "consola": "^3.1.0",
    "defu": "^6.1.2",
    "ofetch": "^1.1.0",
    "semver": "^7.5.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/node": "^18.16.16",
    "@types/semver": "^7.5.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "jiti": "^1.18.2",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1"
  },
  "packageManager": "pnpm@8.6.1"
}
